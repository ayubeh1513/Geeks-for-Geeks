class Solution {
    public char nonRepeatingChar(String s) {
        // code here
        Map<Integer, Integer> m = new HashMap<>();
        for(char i : s.toCharArray()){
            if(m.containsKey(i-'a')){
                m.put(i-'a', m.get(i-'a')+1);
            } else {
                m.put(i-'a', 1);
            }
        }
        for(char i : s.toCharArray()){
            if(m.get(i-'a')==1){
                return i;
            }
        }
        return '$';
    }
}
